.jd-product .jd-secondKill .sk-product ul li p {
	text-align: center;
	font-family: arial;
}
.jd-product .jd-secondKill .sk-product ul li .new-price {
	color:#F23023;
	font-size: 16px;
	font-weight: bold;
	margin-top: 6px;
}
.jd-product .jd-secondKill .sk-product ul li .old-price {
	color:#686868;
	text-decoration: line-through;
}

/*排行榜和闪购*/
.jd-product .content-hotProducts {
    width: 100%;
    border-top: 1px solid #f0f0f0;
    overflow: hidden;
    height: 90px;
}

.jd-product .content-hotProducts .topList {
    padding-top: 7px;
    padding-left: 10px;
    width: 50%;
    float: left;
    border-right: 1px solid #f0f0f0;
    height: auto;
}

.jd-product .content-hotProducts .quickShopping {
    padding-top: 7px;
    padding-left: 10px;
    width: 50%;
    float: right;
}

.jd-product .content-hotProducts .title-txt {
    width: 55%;
    float: left;
}

.jd-product .content-hotProducts .title-txt .txt-head {
    font-size: 16px;
}

.jd-product .content-hotProducts .txt-img {
    width: 45%;
    float: right;
    height: 100%;
}

.jd-product .content-hotProducts .txt-img img {
    width: 100%;
    height: 100%;
}

/*爱生活*/
.jd-product .content-floor {
    width: 100%;
    clear: both;
    background-color: #fff;
}

.jd-product .content-floor .floor-title {
    height: 42px;
    width: 100%;
    position: relative;
    text-align: center;
    vertical-align: top;
}

.jd-product .content-floor .floor-title .floor-txt {
    width: 100%;
    height: 73.6%;
    position: absolute;
    top: 0;
    left: 0;
    display: block;
    margin-top: 5px;
}

.jd-product .content-floor .floor-title img {
    width: 100%;
    height: 100%;
    display: block;
}

.jd-product .content-floor .floor-title .floor-txt img {
    width: auto;
    height: 100%;
    margin: 0 auto;
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
}

.jd-product .content-floor .floor-container {
    width: 100%;
    border-bottom: 1px solid #f0f0f0;
    overflow: hidden;
}

.jd-product .content-floor .floor-container .container-box {
    width: 50%;
    padding: 7px 0 0 10px;
}

.jd-product .content-floor .floor-container .box-left {
    float: left;
    border-right: 1px solid #f0f0f0;
}

.jd-product .content-floor .floor-container .box-right {
    float: right;
}

.jd-product .content-floor .floor-container .container-txt {
    width: 55%;
    float: left;
}

.jd-product .content-floor .floor-container strong {
    font-size: 16px;
    display: block;
    // line-height: 22px;
}

.jd-product .content-floor .floor-container p {
    color: #686868;
    margin: 0;
}

.jd-product .content-floor .floor-container .icon-badge::after {
    content: '年度好货盘点';
    background-color: #ff4a7d;
    color: #fff;
    font-size: 12px;
    text-align: center;
    height: 16px;
    line-height: 16px;
}

.jd-product .content-floor .floor-container .container-img {
    float: right;
    width: 45%;
    padding-right: 6px;
}

.jd-product .content-floor .floor-container img {
    width: 100%;
    height: 100%;
}

.jd-product .content-floor .floor-container .container-box .container-box-child {
    width: 50%;
    padding: 7px 5px 0 10px;
}

.jd-product .content-floor .floor-container .container-box .box-child-left {
    float: left;
    border-right: 1px solid #f0f0f0;
}

.jd-product .content-floor .floor-container .container-box .box-child-right {
    float: right;
}

.jd-product .content-floor .floor-container .container-box .container-box-child .container-box-txt {
    width: 100%;
}

.jd-product .content-floor .floor-container .container-box .container-box-child .container-box-img {
    padding: 0 2px;
    height: 50%;
}

.jd-product .content-floor .floor-container .noPadding {
    padding: 0;
}

/*底部导航*/
.bottom-bar-panel {
	position: fixed;
	bottom:0;
	left:0;
	z-index: 100;
	width: 100%;
	height: 54px;
	background-color: #FFFFFF;
	margin: 0 auto;
}
.bottom-bar-panel .panel-box {
	/*min-width: 300px;
	max-width: 640px;*/
	position:relative;
	height: 54px;
}
.bottom-bar-panel ul {
	width: 100%;
	overflow: hidden;
	height: 100%;
}
.bottom-bar-panel ul li {
	float: left;
	width: 25%;
	text-align: center;
	height: 100%;
}
.bottom-bar-panel ul li a {
	display: inline-block;
	width: 70px;
	height: 100%;
}
.bottom-bar-panel ul li a img {
	width: 100%;
	height: 100%;
}
